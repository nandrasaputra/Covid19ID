<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_menu_item"
    app:startDestination="@id/homeFragment"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.nandra.covid19id.ui.home.HomeFragment"
        android:label="HomeFragment" >
        <action
            android:id="@+id/action_homeFragment_to_indonesiaProvinceDetailFragment"
            app:destination="@id/indonesiaProvinceDetailFragment" />
        <action
            android:id="@+id/action_homeFragment_to_otherCountriesDetailFragment"
            app:destination="@id/otherCountriesDetailFragment" />
    </fragment>

    <fragment
        android:id="@+id/indonesiaProvinceDetailFragment"
        android:name="com.nandra.covid19id.ui.home.indonesiaprovincedetail.IndonesiaProvinceDetailFragment"
        android:label="IndonesiaProvinceDetailFragment" />

    <fragment
        android:id="@+id/otherCountriesDetailFragment"
        android:name="com.nandra.covid19id.ui.home.othercountriesdetail.OtherCountriesDetailFragment"
        android:label="OtherCountriesDetailFragment" />


</navigation>